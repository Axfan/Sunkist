{
    "extends": ["airbnb"],
    "env": {
      "browser": true
    },
    "settings": {
      "import/core-modules": ["electron", "tailwindcss", "autoprefixer"]
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "global-require": "warn",
      "no-unused-vars": "warn",
      "no-undef": "warn",
      "jsx-a11y/alt-text": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "linebreak-style": "off"
    }
  }